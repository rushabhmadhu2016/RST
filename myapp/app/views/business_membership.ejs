<!DOCTYPE html>
 <html lang="en">
    <head>
        <% include frontend/template/head %>
    </head>
    <body>
        <% include frontend/template/subnav %>
        <section class="testimonial-section text-justify">
            <div class="container">
                <div class="row">
                    <div class="col-md-10 offset-md-1">
                       <div class="panel panel-default listing-page-inner">
                         <div class="panel-heading">Business Membership</div>
                           <div class="panel-body">
                             <% include frontend/template/error %>
                             <% i=1; %>
                                <p class="text-center"> You are on Free tier membership.</p>
                                <table class="table table-striped table-bordered table-hover table-sm">
                                <thead class="thead-dark">
                                    <tr><th>#</th><th>Plan Title</th>
                                        <th>Plan Description</th>
                                        <th>Amount </th>
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>                                
                                <% plans.forEach(function(plan){ %>
                                <tr>
                                <th><%=i++%></th>
                                <th><%=plan.title%></th>
                                <th><%=plan.description%></th>
                                <th><%=plan.amount+' '+plan.currency%></th>
                                <th><a href="business-plans/<%=plan._id%>/purchase" class="btn btn-primary"> Purchase Now</a> </th>
                                <th></th>
                                </tr>
                                <% }); %>
                                <tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-12">
                        
                    </div>
                </div>
            </div>
        </div>
        </section> 
   <!-- Footer -->
   <% include frontend/template/subfooter %>
   <script src="/js/validation/profilevalidation.js"></script>
   </body>
 </html> 