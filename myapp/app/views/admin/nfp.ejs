<% include template/subheader %>
<div class="content-wrapper">
  <!-- Page header -->
  <div class="page-header page-header-default">
    <div class="page-header-content">
      <div class="page-title">
        <h4><i class="icon-arrow-left52 position-left"></i> <span class="text-semibold">Home</span> - Non Functional Prototype</h4>
      </div>
    </div>

    <div class="breadcrumb-line">
      <ul class="breadcrumb">
        <li><a href="home"><i class="icon-home2 position-left"></i> Home</a></li>
        <li class="active"><a href="nfp"><i class="icon-design position-left"></i>Non Functional Prototype</a></li>
      </ul>
    </div>
  </div>
  <!-- /page header -->

  <!-- Content area -->
  <div class="content">

    <!-- Dashboard content -->
    <div class="panel panel-flat panel-admin">
        <div class="panel-body">
            <div class="alert alert-danger alert-styled-right alert-bordered">
              <button type="button" class="close" data-dismiss="alert"><span>×</span><span class="sr-only">Close</span></button>
              <span class="text-semibold">Oops..</span> Category linked with locations</a>.
            </div>

            <div class="alert alert-success alert-styled-right alert-arrow-right alert-bordered">
                <button type="button" class="close" data-dismiss="alert"><span>×</span><span class="sr-only">Close</span></button>
                <span class="text-semibold"></span> Blog Submitted Successfully
            </div>

            <form name="emails_form" method="post" action="">
              <div class="form-group">
                <label for="title">Email Name</label>
                <input type="text" name="email_name" id="title" class="form-control" placeholder="Enter email name" value="">
              </div>

              <div class="form-group ck_editerror">
                <label for="email_message">Editor For Dynamic Mail</label>
                <textarea name="email_message" id="email_message" class="form-control" rows="5" placeholder="Email Message"></textarea>
              </div>

              <div class="form-group pt-15">
                    <label class="display-block text-semibold">Left stacked styled</label>
                    <div class="checkbox">
                      <label>
                        <div class="checker"><span class="checked"><input type="checkbox" class="styled" checked="checked"></span></div>
                        Checked styled
                      </label>
                    </div>

                    <div class="checkbox">
                      <label>
                        <div class="checker"><span><input type="checkbox" class="styled"></span></div>
                        Unchecked styled
                      </label>
                    </div>

                    <div class="checkbox disabled">
                      <label>
                        <div class="checker disabled"><span><input type="checkbox" class="styled" disabled="disabled"></span></div>
                        Disabled styled
                      </label>
                    </div>
                  </div>

              <!--<div class="form-group">
                <div class="checkbox checkbox-switchery switchery-lg switchery-double">
                    <label>
                      Is user is qualified for Professional Badge.?
                      <input type="checkbox" class="switchery" checked="checked" value="0">
                    </label>
                </div>
              </div>-->

              
              <input type="submit" name="submit_btn" value="Add" class="btn btn-success">
              <a href="emails" class="btn btn-primary">Cancel</a>
            </form>
        </div>
    </div>
    
    <!-- /dashboard content -->
