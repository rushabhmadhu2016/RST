    <script type="text/javascript">
     jQuery("#category-select").selectmenu();
        jQuery(function(){
            jQuery(".video-player").YouTubePopUp();
            jQuery(".video-player").YouTubePopUp( { autoplay: 0 } ); // Disable autoplay
            $(".owl-carousel").owlCarousel({
                items:2,
                loop:true,
                autoplay:true,
                margin:10,
                autoplayTimeout:5000,
                autoplayHoverPause:true,
                dots:true,
                responsiveClass:true,
                    responsive:{
                    0:{
                        items:1,
                        nav:false
                    },
                    768:{
                        items:2,
                        nav:false
                    },
                    1080:{
                        items:2,
                        nav:false
                    },
                    1600:{
                        items:2,
                        nav:false
                    },
                }
            });   
        });
        $('.img-hover').hover(
            function(){
                $(this).addClass('color-white');
            },
            function(){
                $(this).removeClass('color-white');
            }
        );
        $(window).scroll(function(){
            var sticky = $('#navbar'),
            scroll = $(window).scrollTop(),
            body = $('body');
            if (scroll >= 5){
                sticky.addClass('navbar-fixed');
                body.addClass('navbar-fixed');
            }
            else {
                sticky.removeClass('navbar-fixed');
                body.removeClass('navbar-fixed');
            }
        });
        $('.footer-top').click(function() {
            $('html, body').animate({
                scrollTop: 0
            }, 800);
            return false;
        }); 
    </script>
    <script type="text/javascript">
    function logout(){
            swal({
                title: "Are you sure you want to Logout?",
                icon: "warning",
                //text:"Are you sure want to Logout..?",
                buttons: true,
                buttons: ["No", "Yes"],
                dangerMode: true,
                closeOnClickOutside: false,
                closeOnEsc: false,
                timer: 10000,
            })
            .then((willDelete) => {
                if (willDelete) {
                    document.getElementById('logout-form').submit();
                }
            });
        }   

</script>
