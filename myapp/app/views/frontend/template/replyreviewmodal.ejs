<div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-body">
        <div class="container">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
              <div class="row">
                <form method="post" action="/postreply" name="replyReviewform" id="replyReviewform" enctype="multipart/form-data">
                  <input type="hidden" name="propertyId" id="propId" value="<%=property.id%>">
                      <div class="review-title">
                        <h2 class="edit-title text-center">Reply Review</h2>
                      </div>
                      <div class="section-seprator"><span class="review-titles" id="res_msg"></span></div>
                      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12  edit-description">
                        <span class="post-title">Description</span>
                        <textarea name="review_reply" id="review_reply"></textarea>
                      </div>
                      <div class="edit-media-section">
                        <div class="row">
                            <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12 edit-add-media">
                              <span class="post-title edit-add"></span>
                              <label for="files">
                                 
                              </label>
                           </div>
                            <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                              <button type="submit" id="saveReview" class="save-btn info-btn  pull-right">REPLY</button>
                           </div>
                        </div>    
                    </form>
               </div>
      </div>
    </div>
  </div>
</div>
  